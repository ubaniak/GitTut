<!-- Tab Panes -->
<div class="tab-content">
    <div class="tab-pane active" id="command-line_git_remote">
        Add a remote
        <div id="window">
            <div id="body">
                <p>git remote add &lt;remote name&gt; &gt;url&gt;</p>
                <p> Example </p>
                <p>git remote add origin http://git/git.git</p>
            </div>
        </div>

        Show all remotes
        <div id="window">
            <div id="body">
                <p>git remote -v</p>
            </div>
        </div>

        Delete a remote
        <div id="window">
            <div id="body">
                <p>git remote remove &lt;new name&gt;</p>
            </div>
        </div>

        Delete a branch from a remote
        <div id="window">
            <div id="body">
                <p>git push &lt;remote&gt; :&lt;branch-name&gt;</p>
                <p>OR</p>
                <p>git push &lt;remote&gt; --delete &lt;branch-name&gt;</p>
            </div>
        </div>
    </div>
